{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es5",
        "lib": ["es2018", "dom"],
        "allowSyntheticDefaultImports": true,
        "noEmitOnError": true,
        "downlevelIteration": true
    },
    "include": [
        "./src/**/*",
    ]
}